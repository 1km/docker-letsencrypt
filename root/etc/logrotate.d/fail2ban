/config/log/fail2ban/fail2ban.log {
    weekly
    rotate 7
    missingok
    compress
    delaycompress
    nodateext
    postrotate
        /usr/bin/fail2ban-client set logtarget /config/log/fail2ban/fail2ban.log 1>/dev/null || true
        /usr/bin/fail2ban-client flushlogs
    endscript
    su abc abc
}
